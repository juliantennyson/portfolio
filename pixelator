     <script>

        $(document).ready(function(){
            
             //PARALLAX AND NEXT PROJECT FUNCTION
           $('#content').scroll(function(){
                var x = 100 * this.scrollTop / (this.scrollHeight-this.clientHeight);
                var y = ((-x)/2);
               $('.parallax').css('background-position-y', y);
               if (x > 89){
                $("#nextproj").removeClass("initialH");
                }
                
                if (x <= 89){
                $("#nextproj").addClass("initialH");
                }
            });
            
                
            
            
        });
    
    function loader(){
                $("#loader").fadeOut(2000);
            };
    </script>
